{% extends "base-game.html.twig" %}

{% block stylesheets %}
    {{ parent() }}

    {{ encore_entry_link_tags( 'js/backgammon', null, 'GamePlatform_AngularJs' ) }}
{% endblock %}

{% block javascripts %}
	{{ encore_entry_script_tags( 'js/backgammon', null, 'GamePlatform_AngularJs' ) }}
{% endblock %}

{% block content %}
    <div class="container text-center" style="height: 100%">
        <app-backgammon urlLoginBySignature="{{ urlLoginBySignature }}"></app-backgammon>
    </div>
{% endblock %}
